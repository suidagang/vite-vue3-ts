{
  "name": "vite-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env --max_old_space_size=4096 vite",
    "build": "npm run lint && cross-env vite build",
    "preview": "vite preview",
    "preview:build": "npm run build && vite preview",
    "clean:cache": "rm -rf node_modules && rm -rf .eslintcache && npm run install",
    "lint:prettier": "prettier --write \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "eslint . --ext .ts,.vue,.js,.tsx",
    "mock": "json-server --watch ./src/jsonServer/data.json",
    "commit": "cz"
  },
  "dependencies": {
    "@element-plus/icons": "0.0.11",
    "@vueuse/core": "^7.7.1",
    "animate.css": "^4.1.1",
    "axios": "^0.26.0",
    "echarts": "^5.3.1",
    "element-plus": "^2.0.4",
    "gsap": "^3.9.1",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.11",
    "vue": "^3.2.25",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/echarts": "^4.9.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "add": "^2.0.6",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
